{
  "name": "tarscoin",
  "version": "1.0.0",
  "description": "A coin to save all opies",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "test:grep": "hardhat test --grep $npm_config_grep",
    "format:contracts": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol'",
    "deploy:local": "npx hardhat run scripts/deploy.ts --network base-local",
