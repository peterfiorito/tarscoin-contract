    "deploy:mainnet": "npx hardhat run scripts/deploy.ts --network base-mainnet"
  },
  "keywords": [
    "base",
    "contract",
    "solidity"
  ],
  "author": "me",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.2",
    "hardhat": "^2.22.2",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solidity-coverage": "^0.8.11",
    "typechain": "^8.3.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1"
  }
